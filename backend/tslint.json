{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-single"
    ],
    "trailing-comma": [
      true,
      "never"
    ],
    "only-arrow-functions": false,
    "no-console": false
  },
  "rulesDirectory": []
}